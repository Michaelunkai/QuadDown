{
  "name": "QuadDown",
  "version": "10.1.1",
  "main": "electron/app.js",
  "description": "Skip extracting files, navigating setup processes, or any unnecessary hassles.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuadDown/QuadDown.git",
    "baseUrl": "https://github.com/QuadDown/QuadDown"
  },
  "license": "CC-BY-NC-1.0",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "author": "tagoWorks",
  "keywords": [
    "electron",
    "vite",
    "setup",
    "installer",
    "automation",
    "onboarding",
    "user-experience",
    "cross-platform",
    "desktop-app",
    "productivity",
    "hassle-free",
    "quick-start",
    "packaging",
    "software-launcher",
    "configuration"
