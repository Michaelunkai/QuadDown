{
  "name": "QuadDown-achievement-watcher",
  "version": "1.0.0",
  "description": "Achievement watcher for QuadDown",
  "main": "src/watchdog.js",
  "scripts": {
    "start": "node src/watchdog.js",
    "build-linux": "pkg src/watchdog.js --target node18-linux-x64 --output dist/QuadDownAchievementWatcher",
    "build-win": "pkg src/watchdog.js --target node18-win-x64 --output dist/QuadDownAchievementWatcher.exe",
    "build-mac": "pkg src/watchdog.js --target node18-macos-x64 --output dist/QuadDownAchievementWatcher"
  },
  "dependencies": {
    "find-up": "^4.1.0",
    "ini": "^4.1.1",
    "moment": "^2.29.1",
    "node-watch": "^0.7.4",
    "regedit": "^5.1.4",
    "single-instance": "^0.0.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "assets": [
      "src/**/*",
      "node_modules/regedit/vbs/**/*"
    ],
    "scripts": [
      "node_modules/regedit/**/*.js"
    ],
    "outputPath": "dist"
  },
  "author": "tagoWorks",
  "license": "CC-BY-NC-1.0"
}
